<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" fillWidth="false" spacing="15.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.historian.IndividualPhoto">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <ListView prefHeight="400.0" prefWidth="421.0" />
    <HBox alignment="CENTER" spacing="15.0">
        <DatePicker promptText="Date" />
        <Button text="location" />
        <Button text="Tag" />
        <Button text="Finish" />
        <Button fx:id="BackButton" onAction="#onBackButtonClick" text="Back" />
    </HBox>

</VBox>
